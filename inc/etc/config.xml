        <sales_email_shipment_guest_template>
            <group>sales_shipment</group>
            <conf>sales_email/shipment/guest_template</conf>
            <order>1</order>
            <shipment>1</shipment>
        </sales_email_shipment_guest_template>
        <sales_email_shipment_template>
            <group>sales_shipment</group>
            <conf>sales_email/shipment/template</conf>
            <order>1</order>
            <shipment>1</shipment>
        </sales_email_shipment_template>

        <!-- Invoice -->
        <sales_email_invoice_comment_guest_template>
            <group>sales_invoice</group>
            <conf>sales_email/invoice_comment/guest_template</conf>
            <order>1</order>
            <invoice>1</invoice>
        </sales_email_invoice_comment_guest_template>
        <sales_email_invoice_comment_template>
            <group>sales_invoice</group>
            <conf>sales_email/invoice_comment/template</conf>
            <order>1</order>
            <invoice>1</invoice>
        </sales_email_invoice_comment_template>
        <sales_email_invoice_guest_template>
            <group>sales_invoice</group>
            <conf>sales_email/invoice/guest_template</conf>
            <order>1</order>
            <invoice>1</invoice>
        </sales_email_invoice_guest_template>
        <sales_email_invoice_template>
            <group>sales_invoice</group>
            <conf>sales_email/invoice/template</conf>
            <order>1</order>
            <invoice>1</invoice>
        </sales_email_invoice_template>

        <!-- Errors -->
        <checkout_payment_failed_template>
            <group>errors</group>
            <conf>customer/create_account/email_template</conf>
            <order>1</order>
        </checkout_payment_failed_template>
        <currency_import_error_email_template>
            <group>errors</group>
            <conf>currency/import/error_email_template</conf>
        </currency_import_error_email_template>
        <sitemap_generate_error_email_template>
            <group>errors</group>
            <conf>sitemap/generate/error_email_template</conf>
        </sitemap_generate_error_email_template>

        <!-- Share -->
        <sendfriend_email_template>
            <group>share</group>
            <conf>sendfriend/email/template</conf>
        </sendfriend_email_template>
        <wishlist_email_email_template>
            <group>share</group>
            <conf>wishlist/email/email_template</conf>
            <customer>1</customer>
        </wishlist_email_email_template>

        <!-- Others -->
        <contacts_email_email_template>
            <group>others</group>
            <conf>contacts/email/email_template</conf>
        </contacts_email_email_template>
        <oauth_email_template>
